<p id="notice"><%= notice %></p>

<h1>Bandas</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Genero</th>
      <th>Pais de origem</th>
      <th>Membros</th>
      <th>Media rating</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bandas.each do |banda| %>
      <tr>
        <td><%= banda.name %></td>
        <td><%= banda.genero %></td>
        <td><%= banda.pais_de_origem %></td>
        <td><%= banda.membros %></td>
        <td><%= banda.media_rating %></td>
        <td><%= banda.status %></td>
        <td><%= link_to 'Show', banda %></td>
        <td><%= link_to 'Edit', edit_banda_path(banda) %></td>
        <td><%= link_to 'Destroy', banda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Banda', new_banda_path %>
